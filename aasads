local pl = serv:Channel("Players")
local listpl = {}
function getallplayer()
    listpl = {}
    for i,v in pairs(game:GetService("Players"):GetDescendants()) do
        if v:IsA("Player") then
            listpl[i] = v.Name
        end
        
    end
    return listpl
end

local choosepl = pl:Dropdown("Choose Players", getallplayer(), function(a)
    _G.player = a
end)
pl:Button("Player Refresh", function()
    choosepl:Clear()
    updateplayer()
end)
function updateplayer()
    listpl = {}
    for i,v in pairs(game:GetService("Players"):GetDescendants()) do
        if v:IsA("Player") then
            choosepl:Add(v.Name)
        end
    end
end
pl:Toggle("Kill Player Behind",false, function(stage)
    _G.killplayerbehinda = stage
    if stage then
        killplayerbehind()
    end
end)
pl:Toggle("Kill Player", false, function(stage)
    _G.killplayera = stage
    if stage then
        killplayer()
    end
end)
pl:Button("Teleport To Player", function()
    tppl()
end)
