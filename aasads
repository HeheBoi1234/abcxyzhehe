end)
local mics = serv:Channel("Mics")
mics:Button("Click to coby link discord", function()
    setclipboard("https://discord.gg/EBZNud75ZJ")
end)
mics:Button("Fast Mode", function()
    for key, object in pairs(game:GetService("Workspace"):GetDescendants()) do
        if object:IsA("BasePart") then
             object.Material = "SmoothPlastic"
        end
   end
end)
mics:Button("Rejoin",function()
    game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId,game.JobId,game:GetService('Players').LocalPlayer)
end)
mics:Button("Teleport to Hop Server",function()
    local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/2116452049/servers/Public?sortOrder=Asc&limit=100"))
    for i,v in pairs(Servers.data) do
    if v.playing ~= v.maxPlayers then
        game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, v.id)
    end
end
end)
mics:Toggle("AntiAfk",false, function(stage)
    _G.antiafka = stage
    if stage then
        antiafk()
    end
end)
function tppl()
    local ccc = _G.player
    local v = game:GetService("Workspace")[ccc]
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame*CFrame.new(0,0,2)
end
function killplayerbehind()
    spawn(function()
        while wait(0.0000001) and _G.killplayerbehinda do
            local ccc = _G.player
            local v = game:GetService("Workspace")[ccc]
            v:FindFirstChild("HumanoidRootPart").CanCollide = false
            v:FindFirstChild("HumanoidRootPart").Size = Vector3.new(15,15,15)
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v:FindFirstChild("HumanoidRootPart").CFrame*CFrame.new(0,0,10)
        end
    end)
end
function killplayer()
    spawn(function()
        while wait(0.0000001) and _G.killplayera do
            local ccc = _G.player
            local v = game:GetService("Workspace")[ccc]
            v:FindFirstChild("HumanoidRootPart").CanCollide = false
            v:FindFirstChild("HumanoidRootPart").Anchored = true
            v:FindFirstChild("HumanoidRootPart").Size = Vector3.new(15,15,15)
            v:FindFirstChild("HumanoidRootPart").CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,-10)
        end
    end)
end
function autoquest()
    local cc = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(checkquest())
    if cc == nil then
        for i,v in pairs(game:GetService("Workspace"):GetDescendants()) do 
            if _G.mob == "Thief(lvl:5)" then 
                if v.Name == "Big head boy" then 
                    fireclickdetector(v.Head.ClickDetector)
                    fireclickdetector(v.Head.ClickDetector)
                end
            else if _G.mob == "Buggy pirate(Lvl:30)" then 
                if v.Name == "Bob" then
                    fireclickdetector(v.Head.ClickDetector)
                    fireclickdetector(v.Head.ClickDetector) 
                end
            else if _G.mob == "Attacking Noob(Lvl:100)" then 
                if v.Name == "Sad noob" then
                    fireclickdetector(v.Head.ClickDetector)
                    fireclickdetector(v.Head.ClickDetector) 
                end
            else if _G.mob == "Marine(Lvl:200)" then 
                if v.Name == "Injured pirate" then
                    fireclickdetector(v.Head.ClickDetector)
                    fireclickdetector(v.Head.ClickDetector) 
                end
            else if _G.mob == "Luffy(Lvl:1000)" then 
                if v.Name == "That noob" then 
                    fireclickdetector(v.Head.ClickDetector)
                    fireclickdetector(v.Head.ClickDetector)
                end
            end
        end
    end
    end
    end
    end
    end
    end
    

function checkquest()
    if _G.mob == "Thief(lvl:5)" then
        return "Thief!"
    elseif _G.mob == "Buggy pirate(Lvl:30)" then 
        return "Let them pay back!"
    elseif _G.mob == "Attacking Noob(Lvl:100)" then
        return "Annoying noobs...."
    elseif _G.mob == "Marine(Lvl:200)" then
        return "Marines!"
    elseif _G.mob == "Luffy(Lvl:1000)" then 
        return "The Strongest..."
    end
end
function antiafk()
    spawn(function()
        while wait(0.1) and _G.antiafka do
            local vu = game:GetService("VirtualUser")
            game:GetService("Players").LocalPlayer.Idled:connect(function()
                vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
                wait(1)
                vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            end)
        end
    end)
end
function autoequip()
    spawn(function()
            if _G.hquest then
                local Players = game:GetService("Players")
                local player = Players:FindFirstChildOfClass("Player")
                local name = game:GetService("Players").LocalPlayer.Name
                if player and player.Character then
                    local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
                    if humanoid then
                        local tool = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(checkquest())
                        if tool then
                            humanoid:EquipTool(tool)	
                        end
                    end
                end
            else
                autoequipelse()
        end
    end)
end
function autoequipelse()
    spawn(function()
        while wait(0.4) and _G.autoequipa do
            local Players = game:GetService("Players")
            local player = Players:FindFirstChildOfClass("Player")
            local name = game:GetService("Players").LocalPlayer.Name
            if player and player.Character then
                local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    local tool = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(choosetool)
                    if tool then
                        if game:GetService("Workspace"):FindFirstChild(choosetool) then
                            humanoid:EquipTool(tool)
                        else
                            humanoid:EquipTool(tool)	
                        end
                    end
                end
            end
        end
    end)
end
function autofarm()
    spawn(function()
        while wait(0.0000001) and _G.autofarma do
            if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(checkquest()) == nil then
                autoquest()
            end
            local mob = _G.mob
            if _G.bringmob then
                for i,v in pairs(game:GetService("Workspace")["Npcs"]:GetDescendants()) do
                    if v.Name == mob then
                        if v:FindFirstChild("HumanoidRootPart") then
                            v:FindFirstChild("HumanoidRootPart").Size = Vector3.new(15,15,15)
                            v:FindFirstChild("HumanoidRootPart").CanCollide = false
                            v:FindFirstChild("HumanoidRootPart").Anchored = true
                            v:FindFirstChild("HumanoidRootPart").CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,-10)
                        end
                        claimquest()
                    end
                end
            else
                local v = game:GetService("Workspace")["Npcs"][_G.mob]
                if v:FindFirstChild("HumanoidRootPart") then
                    v:FindFirstChild("HumanoidRootPart").Size = Vector3.new(15,15,15)
                    v:FindFirstChild("HumanoidRootPart").CanCollide = false
                    v:FindFirstChild("HumanoidRootPart").Anchored = true
                    v:FindFirstChild("HumanoidRootPart").CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame*CFrame.new(0,0,-10)
                end
                claimquest()
            end
        end
    end)
end
function claimquest()
    if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(checkquest()) then
        if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(checkquest()).ToolTip == "Finished!,Click to get reward!" then
            _G.hquest = true
            autoequip()
            autoattack()
            if not game:GetService("Players").LocalPlayer.Parent:FindFirstChild(checkquest()) then
                if _G.questdone then
                    _G.questdone = false
                    autoquest()
                end
            end
        end
    end
end

function autoattack()
    spawn(function()
        --// Setting \--
local range = 99e9

--// Variable \--
local player = game:GetService("Players").LocalPlayer

--// Script \--
game:GetService("RunService").RenderStepped:Connect(function()
    local p = game.Players:GetPlayers()
    for i = 2, #p do local v = p[i].Character
        if v and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and player:DistanceFromCharacter(v.HumanoidRootPart.Position) <= range then
            local tool = player.Character and player.Character:FindFirstChildOfClass("Tool")
            if tool and tool:FindFirstChild("Handle") then
                tool:Activate()
                for i,v in next, v:GetChildren() do
                    if v:IsA("BasePart") then
                        firetouchinterest(tool.Handle,v,0)
                        firetouchinterest(tool.Handle,v,1)
                    end
                end
            end
        end
    end
end)
    end)
end
function autoattackelse()
    spawn(function()
        while wait(0.3) and _G.autoattacka do
            local state = _G.autoattacka
            local range = 99e9
            local player = game:GetService("Players").LocalPlayer
            local p = game.Players:GetPlayers()
            for i = 2, #p do local v = p[i].Character
                if state then
                    if v and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and player:DistanceFromCharacter(v.HumanoidRootPart.Position) <= range then
                        local tool = player.Character and player.Character:FindFirstChildOfClass("Tool")
                        if tool and tool:FindFirstChild("Handle") then
                            for i,v in next, v:GetChildren() do
                                tool:Activate()
                                if v:IsA("Part") then
                                    firetouchinterest(tool.Handle,v,0)
                                    firetouchinterest(tool.Handle,v,1)
                                end
                            end
                        end
                    end
                end
            end
        end
    end)
end
function autoattackn(num)
    spawn(function()
        while wait() and num do
            local state = _G.autoattacka
            local range = 99e9
            local player = game:GetService("Players").LocalPlayer
            
                local p = game.Players:GetPlayers()
                for i = 2, #p do local v = p[i].Character
                    if state then
                        if v and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") and player:DistanceFromCharacter(v.HumanoidRootPart.Position) <= range then
                            local tool = player.Character and player.Character:FindFirstChildOfClass("Tool")
                            if tool and tool:FindFirstChild("Handle") then
                                tool:Activate()
                                for i,v in next, v:GetChildren() do
                                    if v:IsA("BasePart") then
                                        firetouchinterest(tool.Handle,v,0)
                                        firetouchinterest(tool.Handle,v,1)
                                    end
                                end
                            end
                        end
                    end
                end
        end
    end)
end


