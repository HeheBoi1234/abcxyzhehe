_G.positionbasey = 0
_G.positionbasez = 0
if game:GetService("Players").LocalPlayer.Name == "xphamtrung" then
    game:GetService("Players").LocalPlayer:Kick("Sao dùng script của anh chi em")
end
for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do 
    if v.ClassName == "Script" then 
        for i,b in pairs(v:GetChildren()) do
            if b.Name == "" then
                v:Destroy()
            end
        end
    end
end
game:GetService("StarterGui"):SetCore("SendNotification", {
    Title = "Viet Hub";
    Text = "Welcome "..game:GetService("Players").LocalPlayer.Name;
})

tab:Button("BaseFarm", function()
    local y = math.random(5000,10000)
    local z = math.random(14000, 20000)
    local add = y + 6
    local addz = z + -2
    local a = "ab"..game.Players.LocalPlayer.Name
    if game:GetService("Workspace"):FindFirstChild(a) then
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(_G.positionbasex,_G.positionbasey,_G.positionbasez)
    else
        local safezone = Instance.new("Part")
        safezone.Position = Vector3.new(129.35, y, z)
        safezone.CanCollide = true
        safezone.Anchored = true
        safezone.Name = "ab"..game.Players.LocalPlayer.Name
        safezone.Parent = game:GetService("Workspace")
        safezone.Size = Vector3.new(259.137, 6.377, 272.82)
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(124.97, add, addz)
        _G.positionbasex = 124.97
        _G.positionbasey = add
        _G.positionbasez = addz
    end
end)

function getAllToolInBackpack()
    local tool = game:GetService("Players").LocalPlayer.Backpack:GetChildren()
    local droptool = {}
    for i=1, #tool do
        if tool[i]:IsA("Tool") then
            if tool[i].Name == "Thief!" or tool[i].Name == "Let them pay back!" or tool[i].Name == "Annoying noobs...." or tool[i].Name == "Marines!" or tool[i].Name == "The Strongest..." then
                local a = 1
            else
                droptool[i] = tool[i].Name
            end
        end
    end
    return droptool
end
local choosetool = ""
local weadrop = tab:Dropdown("Choose Weapon", getAllToolInBackpack(), function(currentOption)
    choosetool = currentOption
end)
tab:Button("Weapon Refresh", function()
    weadrop:Clear()
    updatetool()
end)
function updatetool()
    local tool = game:GetService("Players").LocalPlayer.Backpack:GetChildren()
    for i=1, #tool do
        if tool[i]:IsA("Tool") then
            if tool[i].Name == "Thief!" or tool[i].Name == "Let them pay back!" or tool[i].Name == "Annoying noobs...." or tool[i].Name == "Marines!" or tool[i].Name == "The Strongest..." then
                local a = 1
            else
                weadrop:Add(tool[i].Name)
            end
        end
    end
end
tab:Toggle("Auto Equip",false, function(state)
    _G.autoequipa = state
    if state then
        autoequip()
    else
        _G.autoequipa = false
    end
end)
tab:Toggle("Auto Attack",false, function(state)
    _G.autoattacka = state
    if state then
        autoattack()
    else
        _G.autoattacka = false
    end
end)

