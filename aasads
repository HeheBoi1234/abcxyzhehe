local DiscordLib = loadstring(game:HttpGet"https://raw.githubusercontent.com/kickTh/New-Ui/main/discord%20lib%20(1).txt")()

local win = DiscordLib:Window("VietHub")

local serv = win:Server("Steve One Piece", "http://www.roblox.com/asset/?id=6031075938")
local tab = serv:Channel("Farm")
_G.autoattacka = false
_G.autoequipa = false
_G.autofarma = false
_G.killplayerbehinda = false
_G.killplayera = false
_G.autobuyfruit = false
_G.mob = ""
